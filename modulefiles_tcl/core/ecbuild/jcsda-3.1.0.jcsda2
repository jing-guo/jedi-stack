proc ModulesHelp { } {
        puts stderr "Load ecbuild"
}


set name ecbuild
set ver jcsda-3.1.0.jcsda2
set base $env(OPT)/core/$name/$ver

#prereq cmake

module load cmake/3.16.2

prepend-path "PATH" $base/bin

setenv "ECBUILD_PATH" $base
